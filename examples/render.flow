# yaml-language-server: $schema=https://raw.githubusercontent.com/jahvon/flow/HEAD/schemas/flowfile_schema.json
visibility: private
namespace: examples
executables:
  - verb: show
    name: markdown
    render:
      templateFile: template.md
      templateDataFile: template-data.yaml
      params:
        - prompt: What is your name?
          envKey: NAME
        - text: Hi
          envKey: GREETING
