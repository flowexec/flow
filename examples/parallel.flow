# yaml-language-server: $schema=https://raw.githubusercontent.com/jahvon/flow/HEAD/schemas/flowfile_schema.json
visibility: private
namespace: examples
description: This is a flow executable that demonstrates how to use parallel executable types.
tags: [parallel]
executables:
  - verb: watch
    name: parallel
    parallel:
      refs:
        - run examples:with-output
        - run examples:with-input
        - run examples:from-file
